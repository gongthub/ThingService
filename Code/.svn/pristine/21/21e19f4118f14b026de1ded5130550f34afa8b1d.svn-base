using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using Vlinker.SmartHome.Model.DoorLock;

namespace Vlinker.SmartHome.Model.DTO
{
    public class DoorLockOperation
    {
        /// <summary>
        /// 供应商编码
        /// </summary>
        public string SupplierCode { get; set; }
        /// <summary>
        ///  门店编码
        /// </summary>
        public string StoreCode { get; set; }

        /// <summary>
        /// 门锁类型(1、大门，2、内门)
        /// </summary>
        public string DoorType { get; set; }

        /// <summary>
        /// 房间号
        /// </summary>
        public string RoomCode { get; set; }

        /// <summary>
        /// 内门编码
        /// </summary>
        public string SubDoorCode { get; set; }

        /// <summary>
        /// 操作人手机号
        /// </summary>
        public string OperatorPhone { get; set; }

        /// <summary>
        /// 授权/密码 有效开始时间
        /// </summary>
        public DateTime StartTime { get; set; }

        /// <summary>
        /// 授权/密码 有效结束时间
        /// </summary>
        public DateTime EndTime { get; set; }

        /// <summary>
        /// 授权人手机号
        /// </summary>
        public String AuthFrom { get; set; }
        
        /// <summary>
        /// 被授权人手机号
        /// </summary>
        public string AuthoredMobile { get; set; }

        /// <summary>
        /// 被授权人名称
        /// </summary>
        public string AuthoredName { get; set; }

        /// <summary>
        /// 密码
        /// </summary>
        public string Password { get; set; }

        public List<RoomInfo> RoomInfoList { get; set; }

    }
}